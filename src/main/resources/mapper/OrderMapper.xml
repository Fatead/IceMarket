<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.OrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.entity.Order">
        <id column="order_id" property="orderId" />
        <result column="order_money" property="orderMoney" />
        <result column="order_receiver_info" property="orderReceiverInfo" />
        <result column="order_pay_state" property="orderPayState" />
        <result column="order_time" property="orderTime" />
        <result column="user_id" property="userId" />
        <result column="order_items" property="orderItems" />
    </resultMap>

</mapper>
